{
  "name": "sofia-tree",
  "version": "0.0.1",
  "description": "A kind of Radix Tree data structure baptized 'Sofia Tree' in honour to my beloved daughter. A multinode tree with a level for each letter with fast lookup operations.",
  "author": "Jaime Agudo <jaime.agudo.lopez@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "uglifyjs": "~2.3.6",
    "docco": "~0.6.3"
  },
  "main": "sofia-tree.js",
  "devDependencies": {
    "body-parser": "~1.9.1",
    "chai": "^1.9.2",
    "docco": "0.6.x",
    "express": "~4.9.8",
    "mocha": "^2.0.1",
    "sinon": "^1.11.1",
    "supervisor": "~0.6.0",
    "underscore": "~1.7.0",
    "webkit-devtools-agent": "~0.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaimeagudo/sofiatree.git"
  },
  "scripts": {
    "test": "mocha",
    "example": "node example.js",
    "doc": "docco *.js",
    "uglify": "uglifyjs sofia-tree.js -c \"evaluate=false\" -m --source-map sofia-tree-min.map -o sofia-tree-min.js",
    "prepublish": "npm doc && npm build"
  },
  "analyze": true
}
